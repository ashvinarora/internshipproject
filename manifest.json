{
  "name": "Popout",
  "manifest_version": 2,
  "version": "0.01",
  "description": "This Popout Shows a Funny Gif anytime you want",
	"browser_action": {
		"default_title": "Popout HTML",
		"default_popup": "popout.html"
	},

  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/p5@1.3.1/lib/p5.min.js; object-src 'self'"
}
